{
  "name": "blitz-app-with-chakra-ui-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "dev-debug": "NODE_OPTIONS='--inspect' blitz dev",
    "build": "NODE_ENV=production blitz build && blitz prisma migrate deploy",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "prisma:migrate": "blitz prisma migrate dev",
    "prisma:push": "blitz prisma db push",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:prepare": "NODE_ENV=test blitz prisma db push",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "generate:favicon": "real-favicon generate faviconDescription.json faviconData.json public",
    "docker:up": "docker compose up -d",
    "postinstall": "blitz prisma generate"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.13",
    "@chakra-ui/react": "1.6.3",
    "@chakra-ui/theme-tools": "1.1.7",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@formiz/core": "1.0.3",
    "@formiz/validations": "1.0.0",
    "@hookform/devtools": "3.1.0",
    "@hookform/resolvers": "2.5.2",
    "@prisma/client": "~2.24.0",
    "@sendgrid/client": "7.4.3",
    "@sendgrid/mail": "7.4.4",
    "@types/eslint": "7.2.13",
    "@types/prettier": "2.2.3",
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/react-resizable": "1.7.2",
    "@types/react-table": "7.7.0",
    "blitz": "0.36.3",
    "cli-real-favicon": "latest",
    "downshift": "6.1.3",
    "eslint": "7.27.0",
    "framer-motion": "4.1.17",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-seo": "4.24.0",
    "postmark": "2.7.4",
    "prettier": "2.3.0",
    "prettier-plugin-prisma": "0.11.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4",
    "prisma": "~2.24.0",
    "react": "0.0.0-experimental-6a589ad71",
    "react-charts": "2.0.0-beta.7",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-hook-form": "7.7.1",
    "react-icons": "4.2.0",
    "react-query": "3.16.0",
    "react-resizable": "3.0.1",
    "react-table": "7.7.0",
    "react-tiny-virtual-list": "2.2.0",
    "react-use": "17.2.4",
    "typescript": "~4.3.2",
    "zod": "3.1.0"
  },
  "private": true,
  "devDependencies": {}
}
